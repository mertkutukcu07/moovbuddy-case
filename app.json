{
    "expo": {
        "name": "moovbuddy-case",
        "slug": "moovbuddy-case",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/config/icon.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "splash": {
            "image": "./src/assets/config/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./src/assets/config/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "favicon": "./src/assets/config/favicon.png"
        }
    }
}
